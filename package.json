    "@nx/eslint": "19.3.1",
    "@nx/eslint-plugin": "19.3.1",
    "@nx/jest": "19.3.1",
    "@nx/js": "19.3.1",
    "@nx/nest": "19.3.1",
    "@nx/node": "19.3.1",
